      ],
      "files": [
        "rollup.config.js",
        "../../rollup-common.js"
      ],
      "output": [
        "async-directive.js{,.map}",
        "decorators.js{,.map}",
        "directive-helpers.js{,.map}",
        "directive.js{,.map}",
        "html.js{,.map}",
        "experimental-hydrate-support.js{,.map}",
        "experimental-hydrate.js{,.map}",
        "index.js{,.map}",
        "polyfill-support.js{,.map}",
        "static-html.js{,.map}",
        "decorators/*.js{,.map}",
        "directives/*.js{,.map}"
      ]
    },
    "checksize": {
      "command": "rollup -c --environment=CHECKSIZE",
      "dependencies": [
        "build:ts"
      ],
      "files": [
        "rollup.config.js",
        "../../rollup-common.js"
      ],
      "output": []
    },
